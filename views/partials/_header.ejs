<div class="urls__header">
  <h1 class="urls__header-title">TinyApp</h1>
  <div class="urls__user">
    <% if (user === undefined) {%>
      <form action="/login" method="get" class="header__button">
        <button type="submit" class="login-out">Login</button>
      </form>
      <% } else {%>
        <div>
          <h3>Hello <%=user.firstName%>!</h3>
          <p><%=user.email%></p>
        </div>
      <form action="/logout" method="post" class="header__button">
        <button type="submit" class="login-out">logout</button>
      </form>
    <% } %>
    </div>
</div>